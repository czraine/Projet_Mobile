<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/darker_orange"/> <!-- Couleur lorsque le bouton est pressé -->
            <corners android:radius="24dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/orange"/> <!-- Couleur par défaut -->
            <corners android:radius="24dp"/>
        </shape>
    </item>
</selector>
